{
	"name": "samwilson/embed-wikimedia",
	"description": "A WordPress plugin to add support for embedding URLs of Wikimedia projects such as Wikipedia.",
	"type": "wordpress-plugin",
	"license": "GPL-2.0-or-later",
	"config": {
		"platform": {
			"php": "5.6"
		}
	},
	"autoload": {
		"psr-4": {
			"Samwilson\\EmbedWikimedia\\": "src/"
		}
	},
	"require": {
		"ext-simplexml": "*",
		"composer/installers": "v1.7.0"
	},
	"require-dev": {
		"wp-coding-standards/wpcs": "^2.1"
	}
}
